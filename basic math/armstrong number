//cpp code- alias narcissistic number
#include <iostream>
#include <cmath>
using namespace std;
bool isArmstrong(int n) {
    int original = n;
    int sum = 0;
    int numDigits = floor(log10(n)+1); //static_cast<int>(log10(n) + 1);
    while (n > 0) {
        int digit = n % 10;
        int power= pow(digit, numDigits);
        sum+=power;
        n /= 10;
    }
    cout << "Sum: " << sum << endl;
    return sum == original;
}

int main() {
    int N;
    cin >> N;
    if (isArmstrong(N)) {
        cout << "Yes";
    } else {
        cout << "No";
    }
    return 0;
}
==============================================================================================================
N=int(input())
def armNum(N):
    digits=str(N)
    numDigits=len(digits)
    armSum=sum(int(digit)**numDigits for digit in digits)
    return armSum==N
res=armNum(N)
print("Yes" if res else "No")
