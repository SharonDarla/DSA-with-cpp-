#include <iostream>
#include <cmath>
using namespace std;
bool isArmstrong(int n) {
    int original = n;
    int sum = 0;
    int numDigits = floor(log10(n) + 1);
    while (n > 0) {
        int digit = n % 10;
        sum += pow(digit, numDigits);
        n /= 10;
    }
    return sum == original;
}

int main() {
    int X, Y;
    cin >> X >> Y;
    int count = 0;
    for (int i = X; i <= Y; i++) {
        if (isArmstrong(i)) {
            count++;
        }
    }
    cout << count;
    return 0;
}
